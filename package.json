{
  "name": "vite-turborepo",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@6.27.1",
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "deploy:isolate": "npx pnpm-isolate-workspace --src-files-enable --include-root-deps main",
    "deploy:install": "cd apps/main/_isolated_ && mv package-prod.json package.json && pnpm i",
    "deploy:branch": "git-directory-deploy --directory apps/main/_isolated_ --branch deployed",
    "deploy": "run-s build deploy:isolate deploy:install deploy:branch",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel --continue",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "git-directory-deploy": "^1.5.1",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.5.0",
    "pnpm-isolate-workspace": "^0.0.6",
    "prettier": "^2.5.1",
    "turbo": "^1.1.6"
  }
}
